generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  userId     String   @id @default(uuid()) @map("_id") // Custom ID with UUID default
  Id         String
  username   String
  email      String
  img_link   String
  workspaces Int      @default(0)
  LinksNo    Int      @default(0)
  premium    Boolean  @default(false)
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
